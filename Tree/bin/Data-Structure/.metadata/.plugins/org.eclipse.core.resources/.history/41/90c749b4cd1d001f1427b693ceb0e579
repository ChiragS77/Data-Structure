package com.dsa.section1;


public class DsaRough {

	public static int findExtra(int a[], int b[],int n) {
		
		for(int i = 0; i<n; i++) {
			if(a[i]!=b[i]) {
				return i;
			}
		}
		return -1;
	}
	

	public static void main(String[] args) {

		int a[] = {2,4,6,8,9,10,12};
		int b[] = {2,4,6,8,10,12};
		int n = a.length;
		
		int extra = findExtra(a,b,n);
		System.out.println(extra);
	

	}
}
